// TO ADD TWO NUMBERS IF BOTH ARE GIVEN
#include<stdio.h>
main()
{
int a=5,b=6,c;   //we have declare a, b and c
                 //a and b are also defined or initialized with some values
                //a and b are constants here
                //c is variable because it is not initialized or defined with any value
                //a, b and c have taken space in RAM or primary or main memory
c=a+b;          //instruction i.e, sum of a and b is assigned to c
printf("the sum of a and b is:%d",c);   //output printf is going to print message and going to print value which is of integer type and is coming from C
}
